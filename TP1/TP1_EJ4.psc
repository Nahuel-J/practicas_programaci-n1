	//Desarrolle un algoritmo que permita introducir dos números y mostrar la suma de los mismos.
	Definir numero_1, numero_2, total Como Real;
	Escribir " Ingrese dos numeros para obtener el resultado de la suma de ambos: ";
	Escribir " Ingrese el primer numero: ";
	Leer numero_1;
	Escribir " Ingrese el segundo numero: ";
	Leer numero_2;
	total = numero_1 + numero_2;
	Escribir " El total de la suma es: ",total;
